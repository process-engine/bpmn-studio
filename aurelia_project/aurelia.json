{
  "name": "bpmn-studio",
  "type": "project:application",
  "platform": {
    "id": "web",
    "displayName": "Web",
    "output": "scripts",
    "index": "index.html",
    "baseDir": "."
  },
  "transpiler": {
    "id": "typescript",
    "displayName": "TypeScript",
    "fileExtension": ".ts",
    "dtsSource": [
      "./custom_typings/**/*.d.ts"
    ],
    "source": "src/**/*.ts"
  },
  "markupProcessor": {
    "id": "none",
    "displayName": "None",
    "fileExtension": ".html",
    "source": "src/**/*.html"
  },
  "cssProcessor": {
    "id": "sass",
    "displayName": "Sass",
    "fileExtension": ".scss",
    "source": "src/**/*.scss"
  },
  "editor": {
    "id": "vscode",
    "displayName": "Visual Studio Code"
  },
  "unitTestRunner": {
    "id": "karma",
    "displayName": "Karma",
    "source": "test/unit/**/*.ts"
  },
  "paths": {
    "root": "src",
    "resources": "resources",
    "elements": "resources/elements",
    "attributes": "resources/attributes",
    "valueConverters": "resources/value-converters",
    "bindingBehaviors": "resources/binding-behaviors"
  },
  "testFramework": {
    "id": "jasmine",
    "displayName": "Jasmine"
  },
  "e2eTestRunner": {
    "id": "protractor",
    "displayName": "Protractor",
    "source": "test/e2e/src/**/*.ts",
    "dist": "test/e2e/dist/",
    "typingsSource": [
      "typings/**/*.d.ts",
      "custom_typings/**/*.d.ts"
    ]
  },
  "build": {
    "targets": [
      {
        "id": "web",
        "displayName": "Web",
        "output": "scripts",
        "index": "index.html",
        "baseDir": "."
      }
    ],
    "loader": {
      "type": "require",
      "configTarget": "vendor-bundle.js",
      "includeBundleMetadataInConfig": "auto",
      "plugins": [
        {
          "name": "text",
          "extensions": [
            ".html",
            ".css"
          ],
          "stub": true
        }
      ]
    },
    "options": {
      "minify": "stage & prod",
      "sourcemaps": "dev & stage"
    },
    "bundles": [
      {
        "name": "app-bundle.js",
        "source": [
          "[**/*.js]",
          "**/*.{css,html}"
        ]
      },
      {
        "name": "vendor-bundle.js",
        "prepend": [
          "node_modules/bluebird/js/browser/bluebird.core.js",
          "node_modules/aurelia-cli/lib/resources/scripts/configure-bluebird.js",
          "node_modules/requirejs/require.js",
          "node_modules/whatwg-fetch/fetch.js"
        ],
        "dependencies": [
          "aurelia-binding",
          "aurelia-bootstrapper",
          "aurelia-dependency-injection",
          "aurelia-event-aggregator",
          "aurelia-framework",
          "aurelia-history",
          "aurelia-history-browser",
          "aurelia-loader",
          "aurelia-loader-default",
          "aurelia-logging",
          "aurelia-logging-console",
          "aurelia-metadata",
          "aurelia-pal",
          "aurelia-pal-browser",
          "aurelia-path",
          "aurelia-polyfills",
          "aurelia-route-recognizer",
          "aurelia-router",
          "aurelia-task-queue",
          "aurelia-templating",
          "aurelia-templating-binding",
          "text",
          {
            "name": "aurelia-templating-resources",
            "path": "../node_modules/aurelia-templating-resources/dist/amd",
            "main": "aurelia-templating-resources"
          },
          {
            "name": "aurelia-templating-router",
            "path": "../node_modules/aurelia-templating-router/dist/amd",
            "main": "aurelia-templating-router"
          },
          {
            "name": "aurelia-testing",
            "path": "../node_modules/aurelia-testing/dist/amd",
            "main": "aurelia-testing",
            "env": "dev"
          },
          {
            "name": "aurelia-fetch-client",
            "main": "aurelia-fetch-client",
            "path": "../node_modules/aurelia-fetch-client/dist/amd",
            "resources": []
          },
          "jquery",
          {
            "name": "bootstrap",
            "path": "../node_modules/bootstrap/dist",
            "main": "js/bootstrap.min.js",
            "deps": [
              "jquery"
            ],
            "exports": "$",
            "resources": [
              "css/bootstrap.css"
            ]
          },
          {
            "name": "@process-engine/bpmn-js-custom-bundle",
            "path": "../node_modules/@process-engine/bpmn-js-custom-bundle/dist",
            "main": "bpmn-modeler-custom.js",
            "resources": [
              "bpmn-modeler-custom.css",
              "bpmn-modeler-custom.js"
            ]
          },
          {
            "name": "faye",
            "path": "../node_modules/faye",
            "main": "client/faye-browser-min",
            "resources": [],
            "exports": "Faye"
          },
          {
            "name": "font-awesome",
            "path": "../node_modules/font-awesome",
            "main": "css/font-awesome.min.css",
            "resources": [
              "css/font-awesome.min.css"
            ]
          },
          {
            "name": "@process-engine/consumer_client",
            "path": "../node_modules/@process-engine/consumer_client/dist/amd",
            "main": "index.js"
          },
          {
            "name": "addict-ioc",
            "path": "../node_modules/addict-ioc/dist/amd",
            "main": "index.js"
          },
          "node-uuid",
          {
            "name": "bluebird",
            "main": "./js/browser/bluebird",
            "path": "../node_modules/bluebird"
          },
          {
            "name": "debug",
            "path": "../node_modules/debug",
            "main": "./src/browser"
          },
          {
            "name": "fetch-ponyfill",
            "main": "build/fetch-browser",
            "path": "../node_modules/fetch-ponyfill"
          },
          {
            "name": "proxy-polyfill",
            "main": "proxy.min.js",
            "path": "../node_modules/proxy-polyfill"
          },
          {
            "name": "uuid-browser",
            "path": "../node_modules/uuid-browser",
            "main": "index.js"
          },
          "ms",
          "downloadjs",
          "eventemitter2",
          "velocity-animate",
          "tether",
          {
            "name": "aurelia-bootstrap",
            "path": "../node_modules/aurelia-bootstrap/dist/amd",
            "main": "index",
            "resources": [
              "**/*.html"
            ]
          },
          {
            "name": "toastr",
            "path": "../node_modules/toastr",
            "main": "toastr",
            "resources": [
              "build/toastr.min.css"
            ],
            "deps": [
              "jquery"
            ]
          },
          {
            "name": "canvg-browser",
            "path": "../node_modules/canvg-browser",
            "main": "index.js",
            "exports": "canvg"
          },
          {
            "name": "rgbcolor",
            "path": "../node_modules/rgbcolor",
            "main": "index.js"
          },
          {
            "name": "stackblur",
            "path": "../node_modules/stackblur",
            "main": "index.js"
          },
          {
            "name": "xmldom",
            "path": "../node_modules/xmldom",
            "main": "dom-parser.js"
          },
          {
            "name": "spectrum-colorpicker",
            "path": "../node_modules/spectrum-colorpicker",
            "main": "spectrum.js",
            "resources": [
              "spectrum.css",
              "spectrum.js"
            ]
          },
          {
            "name": "aurelia-validation",
            "main": "aurelia-validation",
            "path": "../node_modules/aurelia-validation/dist/amd",
            "resources": []
          },
          {
            "name": "xml-beautifier",
            "main": "index.js",
            "path": "../node_modules/xml-beautifier/dist"
          },
          {
            "name": "repeat-string",
            "main": "index.js",
            "path": "../node_modules/repeat-string"
          },
          {
            "name": "highlight.js",
            "main": "lib/index.js",
            "path": "../node_modules/highlight.js",
            "resources": [
              "styles/github-gist.css"
            ]
          },
          {
            "name": "highlightjs-line-numbers.js",
            "main": "dist/highlightjs-line-numbers.min.js",
            "path": "../node_modules/highlightjs-line-numbers.js",
            "resources": [
              "styles/*.css"
            ]
          },
          {
            "name": "bpmn-studio_property-panel",
            "main": "property-panel-plugin",
            "path": "../node_modules/bpmn-studio_property-panel/dist/amd",
            "resources": [
              "property-panel/resources/property-panel.html",
              "property-panel/resources/property-panel.js",
              "property-panel/resources/property-panel.css",
              "property-panel/resources/styles/registers.css",
              "property-panel/resources/styles/sections.css",
              "property-panel/resources/indextabs/general/general.html",
              "property-panel/resources/indextabs/general/general.js",
              "property-panel/resources/indextabs/general/general.css",
              "property-panel/resources/indextabs/general/service/general.service.js",
              "property-panel/resources/indextabs/general/sections/basics/basics.html",
              "property-panel/resources/indextabs/general/sections/basics/basics.js",
              "property-panel/resources/indextabs/general/sections/basics/basics.css",
              "property-panel/resources/indextabs/general/sections/call-activity/call-activity.html",
              "property-panel/resources/indextabs/general/sections/call-activity/call-activity.js",
              "property-panel/resources/indextabs/general/sections/call-activity/call-activity.css",
              "property-panel/resources/indextabs/general/sections/escalation-event/escalation-event.html",
              "property-panel/resources/indextabs/general/sections/escalation-event/escalation-event.js",
              "property-panel/resources/indextabs/general/sections/escalation-event/escalation-event.css",
              "property-panel/resources/indextabs/general/sections/flow/flow.html",
              "property-panel/resources/indextabs/general/sections/flow/flow.js",
              "property-panel/resources/indextabs/general/sections/flow/flow.css",
              "property-panel/resources/indextabs/general/sections/message-event/message-event.html",
              "property-panel/resources/indextabs/general/sections/message-event/message-event.js",
              "property-panel/resources/indextabs/general/sections/message-event/message-event.css",
              "property-panel/resources/indextabs/general/sections/pool/pool.html",
              "property-panel/resources/indextabs/general/sections/pool/pool.js",
              "property-panel/resources/indextabs/general/sections/pool/pool.css",
              "property-panel/resources/indextabs/general/sections/script-task/script-task.html",
              "property-panel/resources/indextabs/general/sections/script-task/script-task.js",
              "property-panel/resources/indextabs/general/sections/script-task/script-task.css",
              "property-panel/resources/indextabs/general/sections/signal-event/signal-event.html",
              "property-panel/resources/indextabs/general/sections/signal-event/signal-event.js",
              "property-panel/resources/indextabs/general/sections/signal-event/signal-event.css",
              "property-panel/resources/indextabs/forms/forms.html",
              "property-panel/resources/indextabs/forms/forms.js",
              "property-panel/resources/indextabs/forms/forms.css",
              "property-panel/resources/indextabs/forms/sections/basics/basics.html",
              "property-panel/resources/indextabs/forms/sections/basics/basics.js",
              "property-panel/resources/indextabs/forms/sections/basics/basics.css",
              "property-panel/resources/indextabs/extensions/extensions.html",
              "property-panel/resources/indextabs/extensions/extensions.js",
              "property-panel/resources/indextabs/extensions/extensions.css",
              "property-panel/resources/indextabs/extensions/sections/basics/basics.html",
              "property-panel/resources/indextabs/extensions/sections/basics/basics.js",
              "property-panel/resources/indextabs/extensions/sections/basics/basics.css"
            ]
          }
        ]
      }
    ],
    "copyFiles": {
      "node_modules/bootstrap/dist/fonts/glyphicons-halflings-regular.woff2": "bootstrap/fonts",
      "node_modules/bootstrap/dist/fonts/glyphicons-halflings-regular.woff": "bootstrap/fonts",
      "node_modules/bootstrap/dist/fonts/glyphicons-halflings-regular.ttf": "bootstrap/fonts",
      "src/resources/images/default-avatar.png": "src/resources/images"
    }
  }
}
